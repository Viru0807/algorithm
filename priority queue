#include <iostream>
#include <queue>
using namespace std;

int main() {
    priority_queue<int> pq; // Max Heap (default)

    pq.push(10);
    pq.push(30);
    pq.push(20);
    pq.push(5);

    cout << "Top element (Highest Priority): " << pq.top() << endl;

    pq.pop(); // Removes 30 (highest priority)
    cout << "Next top element: " << pq.top() << endl;

    return 0;
}
